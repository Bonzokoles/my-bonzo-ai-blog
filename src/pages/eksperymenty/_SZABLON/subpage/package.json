{
    "name": "mybonzo-subpage",
    "version": "1.0.0",
    "description": "Podstrona MyBonzo - Cloudflare Pages deployment z base /subpage/",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "astro dev --port 4322",
        "start": "astro dev --port 4322",
        "build": "astro check && astro build",
        "preview": "wrangler pages dev ./dist --compatibility-date=2025-10-31 --compatibility-flags=nodejs_compat --port=4322",
        "deploy": "npm run build && wrangler pages deploy ./dist --project-name=mybonzo-subpage",
        "deploy:prod": "npm run build && wrangler pages deploy ./dist --project-name=mybonzo-subpage --branch=main",
        "astro": "astro",
        "check": "astro check",
        "wrangler": "wrangler"
    },
    "dependencies": {
        "astro": "^5.0.0",
        "@astrojs/cloudflare": "^12.0.0"
    },
    "devDependencies": {
        "@astrojs/check": "^0.9.0",
        "typescript": "^5.6.0",
        "wrangler": "^3.80.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}